{
    
    "name": "RS Wiki Redirector",
    "author": "HexedHero",
    "version": "1.0.5",
    "description": "Redirects you to the new RS Wiki!",
    "manifest_version": 3,

    "background":
    {

        "service_worker": "background.js"

    },
    "permissions":
    [

        "webNavigation"

    ],
    "host_permissions":
    [

        "*://runescape.fandom.com/*",
        "*://runescape.wikia.com/*",
        "*://oldschoolrunescape.fandom.com/*",
        "*://oldschoolrunescape.wikia.com/*"

    ],
    "icons":
    {

        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"

    }
    
}
